{
  "extends": [
    "config:base"
  ],
  "packageRules": [{
    "matchUpdateTypes": ["patch", "pin"],
    "matchDepTypes": ["devDependencies", "dependencies"],
    "automerge": true
  }, {
    "matchUpdateTypes": ["minor", "major"],
    "matchDepTypes": ["dependencies"],
    "semanticCommitType": "feat"
  }, {
    "matchDepTypes": ["devDependencies"],
    "prPriority": -1
  }]
}
